FreePushTool_Button.Activated:Connect(function()
	ModdedPush الlocal = Instance.new("أداة")
	ModdedPush.Name = "ModdedPush"
	ModdedPush.RequiresHandle = false
	ModdedPush.TextureId = "rbxassetid://14478599909"
	ModdedPush.ToolTip = "الدفع المعدل"

	local function ActivateTool()
		الجذر الlocal = GetRoot(plr)
		ضربة localة = الفأرة.الهدف
		الشخص الlocal = لا شيء
		إذا ضربت andضربت.الandالد then
			إذا ضرب.Parent:IsA("Model") ثم
				الشخص = اللعبة.اللاعبandن: Get اللاعب من الشخصية (ضربة.الandالد)
			andإلا إذا ضرب.Parent:IsA("Accessory") ثم
				الشخص = اللعبة.اللاعبandن: Get اللاعب من الشخصية (ضرب.الandالد.الandالد)
			end
			if الشخص then
				دفع local = root.CFrame
				التنبؤTP(الشخص)
				الtask.wait(GetPing()+0.05)
				ادفع(الشخص)
				root.CFrame = pushpos
			end
		end
	end

	ModdedPush.Activated:Connect(function()
		أداة التنشيط ()
	end)
	ModdedPush.Parent = plr.Backpack
end)

