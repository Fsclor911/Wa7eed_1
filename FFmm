Serverhop_Button.Activated:Connect(function()
	if httprequest then
		الخandادم الlocalة = {}
		الطلب الlocal = httprequest({عنandان URL = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Desc&limit=100", game.PlaceId)})
		الجسم الlocal = HttpService:JSONDecode(req.Body)
		if الجسم andبيانات الجسم then
			for i, v في التالي, body.data
				if type(v) == "table" and tonumber(v.playing) and tonumber(v.maxPlayers) and v.playing < v.maxPlayers and v.id ~= game.JobId, then
					الجدandل.إدراج(الخandادم, 1, معرف v.)
				end
			end
		end
		if #servers > 0 then
			اللعبة: GetService("TeleportService"): TeleportToPlaceInstance(game.PlaceId, servers[math.random(1, #servers)], plr)
		end
	end
end)

ChatBox_Input.FocusLost:Connect(function()
	الandسائط الlocalة = {[1] = ChatBox_Input.Text,[2] = "الكل"}
	اللعبة: GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
	ChatBox_Input.Text = ""
end)

