	end
end)

القائمة البيضاءTarget_Button.Activated:Connect(function())
	if TargetedPlayer ~= nil فإن
		if table.find(ScriptWhitelist, Players[TargetedPlayer].UserId) then
			for i andv في أزandاج (ScriptWhitelist) افعل
				if v == Players[TargetedPlayer].UserId then
					الجدandل. إزالة (القائمة البيضاء للنص, i)
				end
			end
			SendNotify("النظام معطل", اللاعب المستهدف.." تمت إزالته من القائمة البيضاء.", 5)
		آخر
			الجدandل.إدراج (القائمة البيضاء للنص, اللاعبandن[اللاعب المستهدف].معرف المستخدم)
			SendNotify("النظام معطل", اللاعب المستهدف.." تمت إضافته to القائمة البيضاء.", 5)
		end
	end
end)

