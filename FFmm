--هدف
انقر فandق زر أداة الهدف. زر الماandس 1 انقر: الاتصال (function ())
	local GetTargetTool = Instance.new("أداة")
	GetTargetTool.Name = "ClickTarget"
	GetTargetTool.RequiresHandle = false
	GetTargetTool.TextureId = "rbxassetid://2716591855"
	GetTargetTool.ToolTip = "تحديد الهدف"

	local function ActivateTool()
		الجذر الlocal = GetRoot(plr)
		ضربة localة = الفأرة.الهدف
		الشخص الlocal = لا شيء
		إذا ضربت andضربت.الandالد then
			إذا ضرب.Parent:IsA("Model") ثم
				الشخص = اللعبة.اللاعبandن: Get اللاعب من الشخصية (ضربة.الandالد)
			andإلا إذا ضرب.Parent:IsA("Accessory") ثم
				الشخص = اللعبة.اللاعبandن: Get اللاعب من الشخصية (ضرب.الandالد.الandالد)
			end
			if الشخص then
				تحديث الهدف(الشخص)
			end
		end
	end

	GetTargetTool.Activated:Connect(function()
		أداة التنشيط ()
	end)
	GetTargetTool.Parent = plr.Backpack
end)

FlingTarget_Button.Activated:Connect(function()
	if TargetedPlayer ~= nil فإن
		تغيير لandن التبديل (زر الهدف)
		if FlingTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			if TouchFling_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0) then
				تغيير لandن التبديل (زر اللمس)
			end
			Local OldPos = GetRoot(plr).Position
			تبديل القذف (true)
			كرر الtask.wait
				task.spawn(function()
					التنبؤTP(اللاعبين[اللاعب المستهدف], "آمن")
				end)
				task.wait()
			حتى FlingTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			TeleportTO(OldPos.X,OldPos.Y,OldPos.Z,"pos","safe")
		آخر
			تبديل القذف(false)
		end
	end
end)

ViewTarget_Button.Activated:Connect(function()
	if TargetedPlayer ~= nil فإن
		تغيير لandن التبديل (عرض الزر المستهدف)
