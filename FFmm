الدالة الlocalة ToggleFling(bool)
	task.spawn(دالة())
		if منطقيًا
			السرعة الlocalة = صفر
			يكرر
				task.spawn(function()
					Rالسرعة = GetRoot (plr).السرعة
					احصل على الجذر (plr). السرعة = Vector3.new (math.random(-1500,1500),-250000,math.random(-1500,1500))
					RunService.RenderStepped:انتظر ()
					GetRoot (plr).السرعة = Rالسرعة
				end)
				تشغيل الخدمة.نبض القلب:انتظر()
			حتى TouchFling_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
		آخر
			TouchFling_Button.Ticket_Asset.ImageColor3 = Color3.fromRGB(255,0,0)
		end
	end)
end

