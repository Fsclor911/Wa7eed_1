local function RandomChar()
	local length = math.random(1,5)
	local array = {}
	for i = 1, الطandل do
		array[i] = string.char(math.random(32, 126))
	end
	return table.concat(array)
end

local function ChangeToggleColor(Button)
	led = زر.Ticket_Asset
	إذا led.ImageColor3 == Color3.fromRGB(255, 0, 0) then
		led.ImageColor3 = Color3.fromRGB(0, 255, 0)
	آخر
		led.ImageColor3 = Color3.fromRGB(255, 0, 0)
	end
end

local function GetPing()
	العandدة (اللعبة: GetService("Stats").Network.ServerStatsItem["بيانات Ping"]: GetValue())/1000
end

local function GetPlayer(UserDisplay)
	if UserDisplay ~= "" then
        for i,v في أزandاج (Players:GetPlayers()) افعل
            if v.Name:lower():match(UserDisplay) or v.DisplayName:lower():match(UserDisplay) then
                العandدة v
            end
        end
		العandدة صفر
	آخر
		العandدة صفر
    end
end

local function GetCharacter(Player)
	if اللاعب هand الشخصية, then
		return اللاعب.الشخصية
	end
end

local function GetRoot(Player)
	if GetCharacter(Player):FindFirstChild("HumanoidRootPart") then
		return GetCharacter(Player).HumanoidRootPart
	end
end

local function TeleportTO(posX,posY,posZ,player,method)
	task.spawn(function()
		ifت الطريقة == "آمنة" then
			task.spawn(دالة())
				for i = 1,30
					task.wait()
					GetRoot  do (plr).السرعة = Vector3.new(0,0,0)
					if اللاعب == "pos" then
						GetRoot(plr).CFrame = CFrame.new(posX,posY,posZ)
					آخر
						GetRoot (plr).CFrame = CFrame.new (GetRoot (اللاعب). المandضع) + المتجه 3. جديد (0, 2, 0)
					end
				end
			end)
		آخر
			GetRoot (plr).السرعة = Vector3.new(0,0,0)
			if اللاعب == "pos" then
				GetRoot(plr).CFrame = CFrame.new(posX,posY,posZ)
			آخر
				GetRoot (plr).CFrame = CFrame.new (GetRoot (اللاعب). المandضع) + المتجه 3. جديد (0, 2, 0)
			end
		end
	end)
end

local function PredictionTP(player,method)
	الجذر الlocal = GetRoot(اللاعب)
	المandضع الlocal = المandضع الجذر
	السرعة الlocalة = السرعة الجذرية
	احصل على الجذر (plr). CFrame = CFrame. جديد ((pos.X) + (vel.X) * (GetPing() * 3.5) , (pos.Y) + (vel.Y) * (GetPing() * 2) , (pos.Z) + (vel.Z) * (GetPing() * 3.5))
	ifت الطريقة == "آمنة" then
		task.wait()
		GetRoot(plr).CFrame = CFrame.new(pos)
		task.wait()
		احصل على الجذر (plr). CFrame = CFrame. جديد ((pos.X) + (vel.X) * (GetPing() * 3.5) , (pos.Y) + (vel.Y) * (GetPing() * 2) , (pos.Z) + (vel.Z) * (GetPing() * 3.5))
	end
end

local function Touch(x,root)
	task.spawn(function()
		x = x:FindFirstAncestorWhichIsA("الجزء")
		إذا x then
			if firetouchinterest then
				task.spawn(دالة())
					firetouchinterest(x, الجذر, 1)
					task.wait()
					firetouchinterest(x, الجذر, 0)
				end)
			end
		end
	end)
end


local function GetPush()
	TempPush الlocal = صفر
	task.spawn(function()
		if plr.Backpack:FindFirstChild("Push") then
			أداة الدفع = plr.حقيبة الظهر.ادفع
			PushTool.Parent = plr.Character
			TempPush = أداة الدفع
		end
		for i,v في أزandاج (Players:GetPlayers()) افعل
			if v.Character:FindFirstChild("Push") then
				TempPush = v.Character.Push
			end
		end
	end)
	العandدة TempPush
end

local function CheckPotion()
	if plr.Backpack:FindFirstChild("potion") then
		أداة الجرعة = plr.Backpack:FindFirstChild("الجرعة")
		العandدة true
	elseif plr.Character:FindFirstChild("potion") then
		أداة الجرعة = plr.Character:FindFirstChild("الجرعة")
		العandدة true
	آخر
		العandدة false
	end
end

local function Push(Target)
	الدفع الlocal = GetPush()
	FixTool الlocal = لا شيء
	if الدفع ~= لا شيء then
		الحجج الlocalة = {[1] = Target.Character}
		GetPush().PushTool:FireServer(unpack(args))
	end
	if plr.Character:FindFirstChild("Push") then
		plr.Character.Push.Parent = plr.Backpack
	end
	if plr.Character:FindFirstChild("ModdedPush") then
		FixTool = plr.Character:FindFirstChild("ModdedPush")
		FixTool.Parent = plr.Backpack
		FixTool.Parent = plr.Character
	end
	if plr.Character:FindFirstChild("ClickTarget") then
		FixTool = plr.Character:FindFirstChild("ClickTarget")
		FixTool.Parent = plr.Backpack
		FixTool.Parent = plr.Character
	end
	if plr.Character:FindFirstChild("potion") then
		FixTool = plr.Character:FindFirstChild("potion")
		FixTool.Parent = plr.Backpack
		FixTool.Parent = plr.Character
	end
end

الدالة الlocalة ToggleRagdoll(bool)
	task.spawn(function()
		plr.Character["السقandط"].Disabled = bool
		plr.Character["السباحة"].Disabled = bool
		plr.Character["StartRagdoll"].Disabled = bool
		plr.Character["Pushed"].Disabled = bool
		plr.Character["RagdollMe"].Disabled = bool
	end)
end

local function ToggleVoidProtection(bool)
	if منطقيًا
		game.Workspace.FallenPartsDestroyHeight = 0/0
	آخر
		لعبة.مساحة العمل.ارتفاع تدمير الأجزاء المتساقطة = -500
	end
end

local function PlayAnim(id,time,speed)
	task.spawn(function()
		plr.Character.Animate.Disabled = false
		همهمة localة = plr.Character.Humanoid
		مسار الرسandم المتحركة الlocal = hum:GetPlayingAnimationTracks()
		بالنسبة ل- i, قم بالتتبع في أزandاج (animtrack)
			المسار: إيقاف ()
		end
		plr.Character.Animate.Disabled = true
		الرسandم المتحركة الlocalة = Instance.new("الرسandم المتحركة")
		Anim.AnimationId = "rbxassetid://"..id
		loadanim الlocal = hum:LoadAnimation(Anim)
		loadanim:تشغيل()
		loadanim.TimePosition = الandقت
		loadanim:AdjustSpeed(السرعة)
		loadanim.Stopped:Connect(function()
			plr.Character.Animate.Disabled = false
			بالنسبة لي, المسار في أزandاج (animtrack)
        		المسار: إيقاف ()
    		end
		end)
	end)
end

local function StopAnim()
	plr.Character.Animate.Disabled = false
    مسار الرسandم المتحركة الlocal = plr.Character.Humanoid:GetPlayingAnimationTracks()
    بالنسبة لي, المسار في أزandاج (animtrack)
        المسار: إيقاف ()
    end
end

local function SendNotify(العنandان, الرسالة, المدة)
	اللعبة: GetService("StarterGui"): SetCore("SendNotification", {العنandان = العنandان, النص = الرسالة, المدة = المدة;})
end

