					لعبة.مساحة العمل.الكاميرا الحالية.مandضandع الكاميرا = اللاعبandن[اللاعب المستهدف].الشخصية.البشرية
				end)
				الtask.wait(0.5)
			حتى ViewTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			لعبة.مساحة العمل.الكاميرا الحالية.مandضandع الكاميرا = plr.الشخصية.البشرية
		end
	end
end)

FocusTarget_Button.Activated:Connect(function()
