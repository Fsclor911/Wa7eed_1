plr.CharacterAdded:Connect(function(x)
	الtask.wait(GetPing()+0.1)
	x:WaitForChild("الإنسان")
	if SavedCheckpoint ~= nil فإن
		TeleportTO(SavedCheckpoint.X,SavedCheckpoint.Y,SavedCheckpoint.Z,"pos","safe")
	end
	if PotionDi_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		تغيير لandن التبديل (زر الجرعة)
		SendNotify("النظام معطل","تم تعطيل PotionDick تلقائيًا بسبب إعادة ظهandر شخصيتك",5)
	end
	if PotionFling_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		تغيير لandن التبديل (زر قذف الجرعة)
		SendNotify("النظام معطل","تم تعطيل PotionFling تلقائيًا بسبب إعادة ظهandر شخصيتك",5)
	end
	if AntiRagdoll_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		تغيير لandن التبديل (زر مكافحة راغدandل)
		SendNotify("النظام معطل","تم تعطيل AntiRagdoll تلقائيًا بسبب إعادة ظهandر شخصيتك",5)
	end
	if SpamMines_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		تغيير لandن التبديل (زر SpamMines)
		SendNotify("النظام معطل","تم تعطيل SpamMines تلقائيًا بسبب إعادة ظهandر شخصيتك",5)
	end
	if Fly_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		تغيير لandن التبديل (زر الطيران)
		الطيران = false
		Fly_Pad.Visible = false
		دالة KeyDown:Disconnect()
		KeyUpFunction:Disconnect()
		SendNotify("النظام معطل","تم تعطيل الطيران تلقائيًا بسبب إعادة ظهandر شخصيتك",5)
	end
	x.Humanoid.Died:Connect(function()
		task.spawn(function()
			x["مدفandع"].معطل = false
			x["RagdollMe"].Disabled = false
		end)
	end)
end)

task.spawn(دالة())
	بينما task.wait(10) تفعل
		task.spawn(function()
			Local GuiVersion =loadstring(game:HttpGet("https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/version"))()
			if الإصدار<GuiVersion then
				SendNotify("النظام معطل","أنت لا تستخدم الإصدار الأحدث, يرجى تشغيل البرنامج النصي مرة أخرى.", 5)
				task.wait(5)
				andسيط النظام:تدمير()
				اللعبة: GetService("TeleportService"): TeleportToPlaceInstance(معرف مكان اللعبة, معرف andظيفة اللعبة, plr)
			end
		end)
	end
end)

