loadstring(game:HttpGet("https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/premium"))() -- تحميل النسخة المميزة


--[[ ======================================================
     ✦ Wa7eed UI Styler ✦
     مظهر متandازن داكن بنصandص بيضاء andلمسات رمادية أنيقة
     لا يؤثر على أي andظائف — فقط تجميل بصري
======================================================== ]]

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local lp = Players.LocalPlayer

local function ApplyWa7eedStyle(gui)
    pcall(function()
        if gui:IsA("Frame") or gui:IsA("ImageLabel") or gui:IsA("ImageButton") then
            gui.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
            gui.BorderSizePixel = 0
            gui.BackgroundTransparency = 0.05
        end
        if gui:IsA("TextLabel") or gui:IsA("TextButton") or gui:IsA("TextBox") then
            gui.Font = Enum.Font.GothamBold
            gui.TextColor3 = Color3.fromRGB(240, 240, 240)
            gui.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
            gui.BackgroundTransparency = 0.15
            gui.BorderSizePixel = 0
            gui.TextScaled = true
            gui.TextStrokeTransparency = 0.9
            gui.TextWrapped = true
        end
    end)
end

local function StyleAll(guiParent)
    for _, g in pairs(guiParent:GetDescendants()) do
        ApplyWa7eedStyle(g)
    end
    guiParent.DescendantAdded:Connect(function(g)
        RunService.Heartbeat:Wait()
        ApplyWa7eedStyle(g)
    end)
end

local function InitStyler()
    local playerGui = lp:WaitForChild("PlayerGui", 5)
    if not playerGui then return end
    StyleAll(playerGui)
end

task.spawn(InitStyler)
print("[Wa7eed Styler] تم تطبيق المظهر الأنيق بنجاح.")
