الطيران الlocal = true
deb الlocal = true
السيطرة الlocalة = {f = 0, b = 0, l = 0, r = 0}
local lastctrl = {f = 0, b = 0, l = 0, r = 0}
دالة KeyDownFunction الlocalة = لا شيء
دالة KeyUpFunction الlocalة = لا شيء
Fly_Button.Activated:Connect(function()
	تغيير لandن التبديل (زر الطيران)
	if Fly_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
		الطيران = true
		ifت اللعبة: GetService("UserInputService").TouchEnabled, فعندئذٍ
			Fly_Pad.Visible = true
		end
		الجذع العلandي الlocal = plr.Character.UpperTorso
		السرعة الlocalة = 0
		local function Fly()
			bg local = Instance.new("BodyGyro", UpperTorso)
			bg.P = 9e4
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
			bg.cframe = UpperTorso.CFrame
			local bv = Instance.new("BodyVelocity", UpperTorso)
			bv.velocity = Vector3.new(0,0.1,0)
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
			بلاي أنيم(10714347256,4,0)
			كرر الtask.wait
				plr.Character.Humanoid.PlatformStand = true
				if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0, فإن
					السرعة = السرعة + سرعة الطيران * 0.10
					ifت السرعة > FlySpeed then
						السرعة = FlySpeed
					end
				andإلا if not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) andالسرعة ~= 0 then
					السرعة = السرعة-سرعة الطيران*0.10
					ifت السرعة < 0 فإن
						السرعة = 0
					end
				end
				if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
					lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
				andإلا if (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 andالسرعة ~= 0 then
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
				آخر
					bv.velocity = Vector3.new(0,0.1,0)
				end
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/FlySpeed),0,0)
			حتى لا تطير
			ctrl = {f = 0, b = 0, l = 0, r = 0}
			lastctrl = {f = 0, b = 0, l = 0, r = 0}
			السرعة = 0
			bg:تدمير()
			bv:تدمير()
			plr.Character.Humanoid.PlatformStand = false
		end

		دالة KeyDown = الماandس.KeyDown: الاتصال (دالة (المفتاح)
			if key:lower() == "w" then
				ctrl.f = 1
				PlayAnim(10714177846,4.65,0)
			elseif key:lower() == "s" ثم
				ctrl.b = -1
				PlayAnim(10147823318,4.11,0)
			elseif key:lower() == "a" ثم
				ctrl.l = -1
				PlayAnim(10147823318,3.55,0)
			elseif key:lower() == "d" ثم
				ctrl.r = 1
				PlayAnim(10147823318,4.81,0)
			end
		end)

		KeyUpFunction = الماandس.KeyUp: الاتصال (andظيفة (المفتاح)
			if key:lower() == "w" then
				ctrl.f = 0
				بلاي أنيم(10714347256,4,0)
			elseif key:lower() == "s" ثم
				ctrl.b = 0
				بلاي أنيم(10714347256,4,0)
			elseif key:lower() == "a" ثم
				ctrl.l = 0
				بلاي أنيم(10714347256,4,0)
			elseif key:lower() == "d" ثم
				ctrl.r = 0
				بلاي أنيم(10714347256,4,0)
			end
		end)
		سافر()
	آخر
		الطيران = false
		Fly_Pad.Visible = false
		دالة KeyDown:Disconnect()
		KeyUpFunction:Disconnect()
		إيقاف الرسandم المتحركة
	end
end)

