		end
	end
end)

BenxTarget_Button.Activated:Connect(function()
	if TargetedPlayer ~= nil فإن
		تغيير لandن التبديل (BenxTarget_Button)
		if BenxTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) then
			بلاي أنيم(5918726674,0,1)
			يكرر
				task.spawn(function()
					if not GetRoot(plr):FindFirstChild("BreakVelocity") then
						task.spawn(function()
							TempV الlocal = Velocity_Asset:Clone()
							TempV.Parent = GetRoot(plr)
						end)
					end
					جذر آخر local = GetRoot(اللاعبين[اللاعب المستهدف])
					GetRoot(plr).CFrame =otherRoot.CFrame * CFrame.new(0,0,1.1)
					GetRoot (plr).السرعة = Vector3.new(0,0,0)
				end)
				task.wait()
			حتى BenxTarget_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
			إيقاف الرسandم المتحركة
			if GetRoot(plr):FindFirstChild("BreakVelocity") then
				GetRoot(plr).BreakVelocity:Destroy()
			end
		end
	end
end)

