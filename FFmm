PushTarget_Button.Activated:Connect(function()
	if TargetedPlayer ~= nil فإن
		pushpos الlocal = GetRoot(plr).CFrame
		التنبؤTP(اللاعبين[اللاعب المستهدف])
		الtask.wait(GetPing()+0.05)
		ادفع(اللاعبين[اللاعب المستهدف])
		GetRoot(plr).CFrame = pushpos
	end
end)

